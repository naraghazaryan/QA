JOIN Transactions ON Transactions.TransactionID=DoneTransactions.TransactionID
WHERE DoneTransactions.Currency='AMD' AND DoneTransactions.Date='05/12/2020';</sql><sql name="SQL 2">SELECT Customers.PassportID, DoneTransactions.Amount, DoneTransactions.Currency
